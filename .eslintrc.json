{
  "globals": {
    "__PATH_PREFIX__": true
  },
  "extends": ["react-app", "plugin:react/recommended"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["warn"],
    "react/display-name": "off",
    "react/jsx-key": [
      "warn",
      {
        "checkFragmentShorthand": true,
        "warnOnDuplicates": true
      }
    ]
  }
}
